 Algoritmo "Dimenssões"
 
 var
  comprimento : real
  largura : real
  area : real
  
  inicio
  // entrada de dados
  escreval ("Digite o comprimento")
  leia (comprimento)
  escreval ("Digite a largura")
  leia (largura)
  
  // processamento de dados
  area <- comprimento * largura
  
  // saída de dados
  escreva ("O valor da sua area é de", area)
  
 fimalgoritmo